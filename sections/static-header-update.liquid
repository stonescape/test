<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script>
     
	const header = document.querySelector('.mega-nav-desktop');
	const mobile_header = document.querySelector('.mega-nav-mobile');
    const cart_count = document.querySelector('.site-header-cart--count');
   

	
    

	
	window.onscroll = function(){
		var top = window.scrollY;
		if (top >= 16){
			header.classList.add('active');
            mobile_header.classList.add('active');
            cart_count.classList.add('active');
  	
		}else{
			header.classList.remove('active');
            mobile_header.classList.remove('active');
            cart_count.classList.remove('active');
           
		}
	}
	
 

  
</script>


